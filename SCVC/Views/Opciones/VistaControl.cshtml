@model BuscarPersona
@{
    Layout = "_MenuPrincipal";
}

<div class="jumbotron border border-primary"
    style="margin-top: 2rem; height: 30rem; background-color: white; border-radius: 1rem;">
    <div class="row" style="text-align: center; margin-top: 2rem;">
        <div class="col">
            <form asp-controller="Usuarios" asp-action="AgregarUsuario" method="post">
                <input type="hidden" asp-for="Id" id="Id" value="@ViewBag.Id">
                <input type="hidden" asp-for="Token" id="Token" value="@ViewBag.Token">
                <input type="hidden" asp-for="Nombre" id="Nombre" value="@ViewBag.Nombre">
                <input type="hidden" asp-for="Usuario" id="Usuario" value="@ViewData["Usuario"]">      
                <button type="submit" class="btn btn-primary">
                        <img src="data:image/png;base64," style="width: 11rem; height: 11rem;" alt="Consulta">
                        <h1>Crear Usuario</h1>
                </button>
            </form>
        </div>
        <div class="col">
            <form asp-controller="Usuarios" asp-action="MostrarUsuarios" method="post">
                <input type="hidden" asp-for="Id" id="Id" value="@ViewBag.Id">
                <input type="hidden" asp-for="Token" id="Token" value="@ViewBag.Token">
                <input type="hidden" asp-for="Nombre" id="Nombre" value="@ViewBag.Nombre">
                <input type="hidden" asp-for="Usuario" id="Usuario" value="@ViewData["Usuario"]">                    
                    <button type="submit" class="btn btn-primary">
                        <img src="https://st2.depositphotos.com/1070291/48779/v/600/depositphotos_487791182-stock-illustration-hotel-reception-service-vector-glyph.jpg" style="width: 11rem; height: 11rem;" alt="Consulta Covid">
                        <h1>Ver Usuarios</h1>
                    </button>
            </form>
        </div>
    </div>
</div>